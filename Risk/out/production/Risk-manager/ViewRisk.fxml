<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="400.0" prefWidth="500.0" style="-fx-background-color: white;"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="Controller">
    <VBox>
        <VBox style="-fx-background-color: white" prefWidth="500.0" prefHeight="50.0" alignment="CENTER">
            <Text text="Overview" layoutY="-1.0">
                <Font name="Palatino" size="18.0"/>
            </Text>
        </VBox>
        <VBox alignment="CENTER" fx:id="vBox"/>
    </VBox>
    <Button alignment="CENTER" layoutX="192.0" layoutY="357.0" mnemonicParsing="false"
            style="-fx-background-color: white; -fx-border-color: lightgray;" text="Create new risk"
            onAction="#createRisk" fx:id="create"/>
</AnchorPane>
